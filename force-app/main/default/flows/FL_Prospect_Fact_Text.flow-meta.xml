    <formulas>
        <name>f_IsIndividualType_ById</name>
        <dataType>String</dataType>
        <expression>IF({!ById.RecordTypeId}=&quot;012gK000001oeCzQAI&quot;,&quot;Individual&quot;, &quot;Corporate&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_IsIndividualType_ByName</name>
        <dataType>String</dataType>
        <expression>IF({!Loop_Prospects.RecordTypeId}=&quot;012gK000001oeCzQAI&quot;,&quot;Individual&quot;, &quot;Corporate&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_LooksLikeId</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN({!f_TrimKey})=15, LEN({!f_TrimKey})=18)</expression>
    </formulas>
    <formulas>
        <name>f_TrimKey</name>
