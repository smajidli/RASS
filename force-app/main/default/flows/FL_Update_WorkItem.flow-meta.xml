    <formulas>
        <name>f_CurrentBilled</name>
        <dataType>Number</dataType>
        <expression>IF({!Yes9},BLANKVALUE({!ById.Total_Hours_Billed__c}, 0),BLANKVALUE({!GetByNumber.Total_Hours_Billed__c}, 0))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>f_HasAnyFieldInput</name>
        <dataType>Boolean</dataType>
        <expression>NOT(
  AND(
    ISBLANK({!newStatus}),
    ISBLANK({!newPriority}),
    ISBLANK({!newSubject}),
    ISBLANK({!newDueDate}),
    ISBLANK({!hoursBilledSet}),
    ISBLANK({!hoursBilledDelta}),
    ISBLANK({!hoursLoggedDelta})
  )
)</expression>
    </formulas>
    <formulas>
        <name>f_IsNull_BilledDelta</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!hoursBilledDelta})</expression>
    </formulas>
    <formulas>
        <name>f_IsNull_BilledSet</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!hoursBilledSet})</expression>
    </formulas>
    <formulas>
        <name>f_IsNull_LoggedDelta</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!hoursLoggedDelta})</expression>
    </formulas>
    <formulas>
        <name>f_LooksLikeId</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN({!f_TrimKey})=15, LEN({!f_TrimKey})=18)</expression>
    </formulas>
    <formulas>
        <name>f_ProposedBilled</name>
        <dataType>Number</dataType>
        <expression>IF(
  NOT({!f_IsNull_BilledSet}),
  {!hoursBilledSet},
  IF(
    NOT({!f_IsNull_BilledDelta}),
    {!f_CurrentBilled} + {!hoursBilledDelta},
    {!f_CurrentBilled}
  )
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>f_ProposedBilled_NonNegative</name>
        <dataType>Number</dataType>
        <expression>IF({!f_ProposedBilled} &lt; 0, 0, {!f_ProposedBilled})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>f_TrimKey</name>
        <dataType>String</dataType>
        <expression>TRIM({!recordKey})</expression>
    </formulas>
    <formulas>
        <name>InstanceBaseUrl</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_620}, FIND(&quot;/services&quot;, {!$Api.Partner_Server_URL_620}) - 1)</expression>
    </formulas>
    <formulas>
        <name>LoggedLabel</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!hoursLoggedDelta}),
  &quot;&quot;,
  IF({!hoursLoggedDelta} &lt; 1,
     TEXT(ROUND({!hoursLoggedDelta}*60,0)) &amp; &quot; Minutes&quot;,
     TEXT(ROUND({!hoursLoggedDelta},0)) &amp; &quot; Hour&quot; &amp;
     IF(ROUND({!hoursLoggedDelta},0)=1,&quot;&quot;,&quot;s&quot;)
  )
)</expression>
    </formulas>
    <formulas>
